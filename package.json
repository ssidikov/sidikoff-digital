{
  "name": "sidikoff-digital",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack --hostname 0.0.0.0",
    "dev:mobile": "next dev --turbopack --hostname 0.0.0.0 --port 3000",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "build:check": "pnpm type-check && pnpm lint && pnpm build",
    "preview": "pnpm build && pnpm start",
    "clean": "rimraf .next out dist",
    "clear-cache": "rm -rf .next && npm run clean",
    "analyze": "ANALYZE=true pnpm build",
    "sanity": "sanity start",
    "sanity:build": "sanity build",
    "sanity:deploy": "sanity deploy",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@sanity/client": "^7.8.1",
    "@sanity/image-url": "^1.1.0",
    "@sanity/vision": "^4.1.1",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.540.0",
    "negotiator": "^1.0.0",
    "next": "15.5.4",
    "next-sanity": "^10.0.4",
    "nodemailer": "^7.0.5",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sanity": "^4.1.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.4",
    "@tailwindcss/postcss": "^4",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^20.19.9",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "typescript": "^5"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ]
}
