<!-- SEO Optimization Icon -->
<svg xmlns="http://www.w3.org/2000/svg" width="600" height="400" viewBox="0 0 600 400" fill="none">
  <rect width="600" height="400" fill="#f8fafc"/>
  
  <!-- Background gradient -->
  <defs>
    <linearGradient id="bg" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#3b82f6;stop-opacity:0.1" />
      <stop offset="100%" style="stop-color:#6366f1;stop-opacity:0.2" />
    </linearGradient>
  </defs>
  <rect width="600" height="400" fill="url(#bg)"/>
  
  <!-- Search icon -->
  <circle cx="300" cy="200" r="80" fill="none" stroke="#3b82f6" stroke-width="6"/>
  <path d="365 265 l25 25" stroke="#3b82f6" stroke-width="6" stroke-linecap="round"/>
  
  <!-- Graph bars -->
  <rect x="200" y="280" width="30" height="80" fill="#10b981" opacity="0.8"/>
  <rect x="240" y="260" width="30" height="100" fill="#3b82f6" opacity="0.8"/>
  <rect x="280" y="240" width="30" height="120" fill="#6366f1" opacity="0.8"/>
  <rect x="320" y="220" width="30" height="140" fill="#8b5cf6" opacity="0.8"/>
  <rect x="360" y="200" width="30" height="160" fill="#ec4899" opacity="0.8"/>
  
  <!-- Title -->
  <text x="300" y="50" text-anchor="middle" fill="#1e293b" font-family="Arial, sans-serif" font-size="32" font-weight="bold">SEO Optimization</text>
  <text x="300" y="80" text-anchor="middle" fill="#64748b" font-family="Arial, sans-serif" font-size="18">Boost Your Search Rankings</text>
</svg>
